{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "explorer.compactFolders": false,
  "editor.renderLineHighlight": "gutter",
  "workbench.editor.labelFormat": "short",
  "extensions.ignoreRecommendations": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "breadcrumbs.enabled": true,
  "editor.parameterHints.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  //adiciona o html snippets no reactjs
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "editor.tabSize": 2,
  "pomodoro.workTime": 90,
  "eslint.alwaysShowStatus": true,
  "git.suggestSmartCommit": false,
  "terminal.integrated.defaultProfile.osx": "/bin/zsh",
  //exclui diretórios específicos da área de trabalho do observador de arquivos do VS Code
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.rulers": [
    80,
    120
  ],
  "json.schemas": [],
  "material-icon-theme.folders.associations": {
    "adapters": "contract",
    "domain": "class",
    "dtos": "typescript",
    "entities": "class",
    "eslint-config": "tools",
    "fakes": "mock",
    "factories": "class",
    "grpc": "pipe",
    "http": "container",
    "implementations": "core",
    "infra": "app",
    "kafka": "pipe",
    "kube": "kubernetes",
    "main": "lib",
    "mappers": "meta",
    "middlewares": "middleware",
    "migrations": "tools",
    "modules": "components",
    "presentation": "template",
    "protos": "pipe",
    "protocols": "contract",
    "providers": "include",
    "repositories": "mappings",
    "schemas": "class",
    "seeds": "connection",
    "subscribers": "messages",
    "typeorm": "database",
    "use-cases": "functions",
    "useCases": "functions",
    "view-models": "views",
    "websockets": "pipe",
    "_shared": "shared",
  },
  "material-icon-theme.files.associations": {
    "connection.ts": "capacitor",
    "ormconfig.json": "database",
    "server.ts": "http",
    "tsconfig.json": "tune",
    "*.proto": "proto",
    "*.webpack.js": "webpack",
  },
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.languages.associations": {
    "dotenv": "tune"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.associations": {
    ".env.*": "dotenv",
    "*.jsx": "typescriptreact",
    ".prettierrc": "json",
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    "*.tsx": "typescriptreact",
  },
  "git.autofetch": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true,
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "cSpell.language": "en,pt-BR",
  "cSpell.enableFiletypes": [
    "!asciidoc",
    "!c",
    "!cpp",
    "!csharp",
    "!go",
    "!handlebars",
    "!haskell",
    "!jade",
    "!java",
    "!latex",
    "!php",
    "!pug",
    "!python",
    "!restructuredtext",
    "!rust",
    "!scala",
    "!scss"
  ],
  "workbench.colorTheme": "Omni",
}
